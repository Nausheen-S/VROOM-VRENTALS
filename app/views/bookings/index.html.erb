<body class="customerbookingbody">
<div class="container">
<h3 class="customerbookingheading">Bookings Till Date</h3>
<div class="customerbookingtext">
<% @bookings.each do |booking| %>
  <li class="customerbookinglist">
  	<%=  image_tag(booking.car.image_url) %>
  </li>
	<li class="customerbookinglist">
		<%= form_with scope: :booking, model: @booking, local: true  do |form| %>

 		<!--<p>Are you sure you want to return the car? </p>-->
  	<p class="returnbutton">
  		<%= submit_tag "Return Car" , :class => "btn btn-dark" %>
  	</p>
	</li>
<% end %>
<% end %>
</div>
</div>
</body>
